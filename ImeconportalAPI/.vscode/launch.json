{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "ImeconportalAPI",
            "port": 9229,
            "address": "localhost",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/usr/src/app",
            "protocol": "inspector",
            "command": "npm run start:debug"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: start:debug",
            "request": "launch",
            "command": "npm run start:debug",
            "cwd": "${workspaceFolder}"
        }
    ]
}